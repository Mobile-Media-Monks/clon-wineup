{
  "name": "BaseProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf node_modules/ && yarn cache clean && yarn",
    "clean-ios": "cd ios && rm -rf build/ && xcodebuild clean && cd ..",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "set-development": "cp ./.env.development ./.env",
    "set-staging": "cp ./.env.staging ./.env",
    "set-production": "cp ./.env.production ./.env",
    "ios:development": "yarn set-development && react-native run-ios --mode=Debug --scheme=Development",
    "ios:staging": "yarn set-staging && react-native run-ios --mode=Staging --scheme=Staging",
    "ios:production": "yarn set-production && react-native run-ios --mode=Production --scheme=Production",
    "android:development": "yarn set-development && react-native run-android --mode=devDebug",
    "android:staging": "yarn set-staging && react-native run-android --mode=stagingDebug",
    "android:production": "yarn set-production && react-native run-android --mode=productionDebug",
    "prepare": "husky",
    "commit": "npx git-cz -e",
    "lint-staged-files": "git diff --cached --name-only -- '*.js' '*.jsx' '*.ts' '*.tsx' | xargs eslint --fix && git diff --cached --name-only -- '*.js' '*.jsx' '*.ts' '*.tsx' | xargs prettier --write"
  },
  "dependencies": {
    "@react-navigation/native": "^7.0.7",
    "@react-navigation/native-stack": "^7.1.8",
    "i18next": "^24.0.2",
    "react": "18.3.1",
    "react-i18next": "^15.1.2",
    "react-native": "0.76.2",
    "react-native-config": "^1.5.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^3.3.0",
    "react-native-safe-area-context":
     "^4.14.0",
    "react-native-screens": "^4.3.0",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.15.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.2",
    "@react-native/eslint-config": "0.76.2",
    "@react-native/metro-config": "0.76.2",
    "@react-native/typescript-config": "0.76.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.15.0"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn run lint-staged-files"
    ]
  }
}
